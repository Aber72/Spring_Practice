<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">
<!-- Bean definitions here -->


	<bean class="com.beanautowiring.Address" name ="adress">
	
		<property name="street" value="2nd Street"/>
		<property name="city" value="Jamshedpur"/>
		<property name="state" value="Jharkhand"/>
	
		
	</bean>

	
	<bean class="com.beanautowiring.Emp" name="employee" autowire="byName" />
	

		
	
		
	

	
</beans>